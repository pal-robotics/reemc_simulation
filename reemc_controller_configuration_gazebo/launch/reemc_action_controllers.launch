<launch>

  <!-- Params for PAL motors action controller -->
  <rosparam command="load" file="$(find reemc_controller_configuration_gazebo)/config/reemc_action_controllers.yaml" />


  <!-- Controllers that come up started -->
  <node name="body_controllers_spawner"
        pkg="pr2_controller_manager" type="spawner" output="screen"
        args="--wait-for=/calibrated whole_body_controller" />



</launch>
